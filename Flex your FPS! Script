-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ez = Instance.new("TextButton")
local medium = Instance.new("TextButton")
local hard = Instance.new("TextButton")
local extreme = Instance.new("TextButton")
local fps1 = Instance.new("TextButton")
local fps4 = Instance.new("TextButton")
local fps2 = Instance.new("TextButton")
local fps5 = Instance.new("TextButton")
local fps3 = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(56, 56, 56)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.36520505, 0, 0.341447085, 0)
Frame.Size = UDim2.new(0, 189, 0, 155)
Frame.Active = true
Frame.Draggable = true

ez.Name = "ez"
ez.Parent = Frame
ez.BackgroundColor3 = Color3.fromRGB(0, 255, 30)
ez.BorderColor3 = Color3.fromRGB(0, 0, 0)
ez.BorderSizePixel = 0
ez.Position = UDim2.new(0.049232848, 0, 0.153276801, 0)
ez.Size = UDim2.new(0, 93, 0, 20)
ez.Font = Enum.Font.Michroma
ez.Text = "Complete Easy Obby"
ez.TextColor3 = Color3.fromRGB(0, 0, 0)
ez.TextScaled = true
ez.TextSize = 14.000
ez.TextWrapped = true
ez.MouseButton1Down:connect(function()
	local targetPosition = Vector3.new(-1630.39, 32.05, 2010.47)

	local textButton = script.Parent

	local function teleportPlayer(player)
		local character = player.Character or player.CharacterAdded:Wait()
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				humanoidRootPart.CFrame = CFrame.new(targetPosition)
			end
		end
	end

	textButton.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer
		if player then
			teleportPlayer(player)
		end
	end)
end)

