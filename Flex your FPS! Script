fps1.Name = "fps1"
fps1.Parent = Frame
fps1.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
fps1.BorderColor3 = Color3.fromRGB(0, 0, 0)
fps1.BorderSizePixel = 0
fps1.Position = UDim2.new(0.597803175, 0, 0.153276801, 0)
fps1.Size = UDim2.new(0, 70, 0, 20)
fps1.Font = Enum.Font.Michroma
fps1.Text = "0 FPS"
fps1.TextColor3 = Color3.fromRGB(0, 0, 0)
fps1.TextScaled = true
fps1.TextSize = 14.000
fps1.TextWrapped = true
fps1.MouseButton1Down:connect(function()
	while true do
		local args = {
			[1] = 0
		}
		game:GetService("ReplicatedStorage").UpdateFPS:FireServer(unpack(args))
		wait(0.01)
	end
end)


